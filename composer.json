{
  "name": "aberdeener/LuckPerms-API",
  "description": "PHP wrapper for LuckPerms REST API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tadhg",
      "email": "tadhgsmboyle@gmail.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.4.5",
    "illuminate/collections": "^9.21",
    "illuminate/container": "^9.22"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^6.1",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "LuckPermsAPI\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
